{
  "name": "hull-clearbit",
  "description": "Enrich Customer data with Clearbit",
  "version": "0.2.0",
  "homepage": "https://github.com/hull-ships/hull-clearbit",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-clearbit"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-clearbit/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "./node_modules/.bin/babel server -d build",
    "start": "node -r newrelic ./build",
    "start:dev": "LOG_LEVEL=debug NODE_ENV=development ./node_modules/.bin/nodemon -d 2 -V -w server -x ./node_modules/.bin/babel-node -- server",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "./node_modules/.bin/eslint server",
    "test:modules": "npm outdated --depth=0",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit/*.js",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration/*.js",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf build",
    "update": "./node_modules/.bin/updtr",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "devDependencies": {
    "babel-eslint": "^6.0.5",
    "chai": "^4.1.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "minihull": "^2.0.1",
    "mocha": "^2.5.3",
    "nock": "^9.0.13",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "updtr": "^0.2.1",
    "watch": "^0.19.1"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.0",
    "bottleneck": "1.15.0",
    "clearbit": "1.2.3",
    "connect": "^3.4.1",
    "ejs": "^2.5.7",
    "event-stream": "^3.3.2",
    "express": "^4.13.4",
    "find-up": "^2.1.0",
    "hull": "^0.11.12",
    "jquery": "^3.1.0",
    "jwt-simple": "^0.5.0",
    "librato-node": "^4.1.0",
    "lodash": "^4.11.1",
    "moment": "^2.13.0",
    "newrelic": "^1.28.3",
    "object-mapper": "^3.0.0",
    "range_check": "^1.4.0",
    "rimraf": "^2.5.2",
    "select2": "^4.0.3",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.12.0"
  }
}
