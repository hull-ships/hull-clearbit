    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "newrelic": "^7.1.1",
    "nock": "^9.6.1",
    "object-mapper": "^4.2.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "range_check": "^1.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.1.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
